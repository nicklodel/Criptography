%o no
function valide=knapsack(s)
valide = 1;
sLength = length(s);
for i=1:sLength
    if s(i) ~= floor(s(i))
    valide = -1;
    end
    
end
totalMochila=0;
for i=1:sLength
    for j=1:i-1
        if j == 1
        continue;
        end
        totalMochila = totalMochila + s(j);
    end
  if totalMochila >= s(i)
  valide = 0;
  end
totalMochila = 0;
end
end
